{
    "compilerOptions": {
        "target": "es6",
        "module": "esnext",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext",
        ],
        "types": [
            "@cloudflare/workers-types"
        ],
        "sourceMap": true,
        "esModuleInterop": true,
        "allowJs": true,
        "jsx": "preserve",
        "strict": true,
        "noImplicitAny": false,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "src"
    ]
}